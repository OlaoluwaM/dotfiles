{
  // Workspaces
  "hyprland/workspaces": {
    "on-click": "activate",
    "active-only": false,
    "all-outputs": true,
    "format": "{id}",
    "format-icons": {
      "urgent": "",
      "active": "",
      "default": ""
    }
  },

  // Hyprland Window
  "hyprland/window": {
    "format": "{class}"
  },

  "custom/arrow": {
    "format": "ÔÉô"
  },

  // System tray
  "tray": {
    // "icon-size": 21,
    "spacing": 15
  },

  // Clock
  "clock": {
    // "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
    "format": "{:%A %B %d ~ %H:%M}",
    "on-click": "gnome-calendar"
  },

  "memory": {
    "on-click": "kitty btm --default_widget_type proc -e",
    "interval": 10,
    "format": "Ôî∏ {percentage}%",
    "states": {
      "low": 1,
      "medium": 25,
      "high": 50
    }
  },

  "cpu": {
    "on-click": "kitty btm --default_widget_type cpu -e",
    "interval": 10,
    "format": "Ôãõ {usage}%",
    "tooltip": false,
    "states": {
      "low": 1,
      "medium": 25,
      "high": 50
    }
  },

  // Network
  "network": {
    "format": "{ifname}",
    "format-wifi": "Ôá´  {essid}  {bandwidthDownBytes}",
    "format-ethernet": "ÔÉÅ",
    "format-disconnected": "Ôá´ ", //An empty format will hide the module.
    "tooltip-format": "Ôûñ {ifname} via {gwaddri}",
    "tooltip-format-wifi": "Ôá´  {essid} ({signalStrength}%)",
    "tooltip-format-ethernet": "ÔÉÅ  {ifname} ({ipaddr}/{cidr})",
    "tooltip-format-disconnected": "Ôá´ ",
    "max-length": 50,
    "on-click": "nm-connection-editor"
  },

  // Battery
  "battery": {
    "states": {
      "medium": 45,
      "warning": 20,
      "critical": 5
    },
    "format": "{icon}",
    "format-charging": "ÔÉß {capacity}%",
    "format-plugged": "Óïõ {capacity}%",
    "format-discharging": "{icon} {capacity}%",
    "tooltip-format": "{capacity}%",
    "interval": 10,
    "format-icons": ["ÔâÑ ", "ÔâÉ ", "ÔâÇ ", "ÔâÅ ", "ÔâÄ "]
  },

  // Bluetooth
  "bluetooth": {
    "format": "Ôäî",
    "interval": 30,
    "tooltip-format": "Bluetooth",
    "tooltip-format-connected": "{controller_alias}\t{status}",
    "on-click": "blueman-manager"
  },

  "custom/screencast": {
    "format": "{}",
    "tooltip": true,
    "tooltip-format": "Recording",
    "interval": 1,
    "exec": "echo 'ÔÑë'",
    "exec-if": "pgrep 'wf-recorder'",
    "on-click": "ags -b hypr -r 'recorder.stop()'"
  },

  "custom/vpn": {
    "format": "ÔÄ£",
    "exec": "echo '{\"class\": \"connected\"}'",
    "exec-if": "test -d /proc/sys/net/ipv4/conf/tun0",
    "return-type": "json",
    "interval": 5,
    "tooltip": true,
    "tooltip-format": "VPN"
  },

  "mpris": {
    "format": "{player_icon} {title}",
    "format-paused": "{status_icon} <i>{player} {title}</i>",
    "player-icons": {
      "default": "üéµ"
    },
    "status-icons": {
      "paused": "‚è∏"
    }
    // "ignored-players": ["firefox"]
  }

